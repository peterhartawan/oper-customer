A,IAAA,YA7DA,wBA6DA,WA7DA,EA6DA;AACA,aAAA,UAAA,IAAA,EAAA;AACA,eACA,IAAA,CAAA,KAAA,CAAA,WAAA,GAAA,OAAA,CAAA,WAAA,CAAA,WAAA,EAAA,MAAA,CADA;AAGA,OAJA;AAKA;AAnEA,GA/JA;AAoOA,EAAA,QAAA,kCACA,UAAA,CAAA;AACA,IAAA,KAAA,EAAA,MAAA,CAAA,SADA;AAEA,IAAA,MAAA,EAAA,MAAA,CAAA,UAFA;AAGA,IAAA,aAAA,EAAA,MAAA,CAAA,iBAHA;AAIA,IAAA,IAAA,EAAA,MAAA,CAAA,eAJA;AAKA,IAAA,SAAA,EAAA,MAAA,CAAA,iBALA;AAMA,IAAA,YAAA,EAAA,MAAA,CAAA;AANA,GAAA,CADA;AASA,IAAA,YAAA,EAAA,wBAAA;AACA,aAAA,KAAA,IAAA,CAAA,kBAAA,GAAA,KAAA,MAAA;AACA,KAXA;AAYA,IAAA,QAZA,sBAYA;AACA,aAAA,KAAA,IAAA,IAAA,IAAA,GACA,KAAA,IAAA,CAAA,KAAA,GAAA,KAAA,IAAA,CAAA,KADA,GAEA,KAAA,IAAA,CAAA,KAAA,GAAA,EAFA;AAGA;AAhBA,IApOA;AAsPA,EAAA,OAtPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBAuPA,KAAA,MAAA,CAAA,QAAA,CAAA,MAAA,CAAA,iBAAA,CAvPA;;AAAA;AAuPA,cAAA,IAvPA;AAAA;AAAA,qBAwPA,KAAA,MAAA,CAAA,QAAA,CAAA,MAAA,CAAA,aAAA,CAxPA;;AAAA;AAwPA,cAAA,WAxPA;AAyPA,mBAAA,OAAA,GAAA,IAAA;AACA,mBAAA,cAAA,GAAA,WAAA;AACA,mBAAA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,eAAA,EAAA,IAAA;AACA,mBAAA,MAAA,CAAA,QAAA,CAAA,MAAA,CAAA,aAAA,EAAA,CAAA;;AA5PA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA,CAAA","sourcesContent":["<template>\r\n  <div class=\"page-table scrollable only-y\" id=\"affix-container\">\r\n    <div class=\"card-base card-shadow--medium bg-white\">\r\n      <el-form ref=\"form\" :rules=\"rules\" :model=\"form\">\r\n        <el-row>\r\n          <el-col :span=\"24\">\r\n            <div class=\"grid-content\">\r\n              <el-form-item prop=\"booking_time\">\r\n                <el-date-picker\r\n                  v-model=\"form.booking_time\"\r\n                  type=\"datetime\"\r\n                  placeholder=\"Booking Time\"\r\n                  value-format=\"yyyy-MM-dd HH:mm:ss\"\r\n                  size=\"large\"\r\n                  prefix-icon=\"el-icon-date\"\r\n                >\r\n                </el-date-picker>\r\n              </el-form-item>\r\n            </div>\r\n          </el-col>\r\n        </el-row>\r\n\r\n        <el-row :gutter=\"20\">\r\n          <el-col :span=\"24\">\r\n            <div class=\"grid-content\">\r\n              <el-form-item prop=\"message\">\r\n                <el-input\r\n                  type=\"textarea\"\r\n                  v-model=\"form.message\"\r\n                  placeholder=\"Order Message\"\r\n                  :autosize=\"{ minRows: 5, maxRows: 7 }\"\r\n                ></el-input>\r\n              </el-form-item>\r\n            </div>\r\n          </el-col>\r\n        </el-row>\r\n\r\n        <el-row>\r\n          <el-col :span=\"24\">\r\n            <el-col>\r\n              <p>User</p>\r\n              <hr />\r\n            </el-col>\r\n          </el-col>\r\n        </el-row>\r\n\r\n        <el-row :gutter=\"20\">\r\n          <el-col :span=\"12\">\r\n            <div class=\"grid-content\">\r\n              <el-form-item prop=\"user_fullname\">\r\n                <el-input\r\n                  placeholder=\"Name\"\r\n                  v-model=\"form.user_fullname\"\r\n                ></el-input>\r\n              </el-form-item>\r\n            </div>\r\n          </el-col>\r\n          <el-col :span=\"12\">\r\n            <el-form-item prop=\"user_phonenumber\">\r\n              <el-input\r\n                placeholder=\"Phone\"\r\n                v-on:keypress.native=\"isNumber\"\r\n                v-model=\"form.user_phonenumber\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n\r\n        <el-row>\r\n          <el-col :span=\"24\">\r\n            <el-col>\r\n              <p>Locations</p>\r\n              <hr />\r\n            </el-col>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"20\">\r\n          <el-col :span=\"12\">\r\n            <div class=\"grid-content\">\r\n              <el-form-item prop=\"origin_latitude\">\r\n                <el-select\r\n                  v-model=\"origin\"\r\n                  @change=\"originL\"\r\n                  filterable\r\n                  placeholder=\"Select Origin\"\r\n                >\r\n                  <el-option\r\n                    v-for=\"item in options\"\r\n                    :key=\"item.value\"\r\n                    :label=\"item.name\"\r\n                    :value=\"item\"\r\n                  ></el-option>\r\n                </el-select>\r\n              </el-form-item>\r\n            </div>\r\n          </el-col>\r\n          <el-col :span=\"12\">\r\n            <el-form-item prop=\"destination_latitude\">\r\n              <el-select\r\n                v-model=\"destination\"\r\n                @change=\"destionationL\"\r\n                placeholder=\"Select Destination\"\r\n              >\r\n                <el-option\r\n                  v-for=\"item in options\"\r\n                  :key=\"item.value\"\r\n                  :label=\"item.name\"\r\n                  :value=\"item\"\r\n                ></el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n\r\n        <el-row>\r\n          <el-col :span=\"24\">\r\n            <el-col>\r\n              <p>Vehicle</p>\r\n              <hr />\r\n            </el-col>\r\n          </el-col>\r\n        </el-row>\r\n\r\n        <el-row :gutter=\"20\">\r\n          <el-col :span=\"8\">\r\n            <div class=\"grid-content\">\r\n              <el-form-item prop=\"vehicle_brand\">\r\n                <el-select\r\n                  style=\"width: 100%\"\r\n                  v-model=\"form.vehicle_brand\"\r\n                  @change=\"vehicleBrand\"\r\n                  filterable\r\n                  placeholder=\"Vehicle Brand\"\r\n                >\r\n                  <el-option\r\n                    v-for=\"item in vehicle_brands\"\r\n                    :key=\"item.id\"\r\n                    :label=\"item.brand_name\"\r\n                    :value=\"item.id\"\r\n                  ></el-option>\r\n                </el-select>\r\n              </el-form-item>\r\n            </div>\r\n          </el-col>\r\n          <el-col :span=\"8\">\r\n            <div class=\"grid-content\">\r\n              <el-form-item prop=\"vehicle_type\">\r\n                <el-autocomplete\r\n                  class=\"inline-input\"\r\n                  v-model=\"form.vehicle_type\"\r\n                  :fetch-suggestions=\"querySearch\"\r\n                  placeholder=\"Vehicle Type\"\r\n                  :trigger-on-focus=\"false\"\r\n                  style=\"width: 100%\"\r\n                ></el-autocomplete>\r\n              </el-form-item>\r\n            </div>\r\n          </el-col>\r\n          <el-col :span=\"8\">\r\n            <div class=\"grid-content\">\r\n              <el-form-item prop=\"vehicle_transmission\">\r\n                <el-select\r\n                  v-model=\"form.vehicle_transmission\"\r\n                  style=\"width: 100%\"\r\n                  placeholder=\"Select Vehicle Transmission\"\r\n                >\r\n                  <el-option label=\"Manual\" value=\"Manual\"></el-option>\r\n                  <el-option label=\"Automatic\" value=\"Automatic\"></el-option>\r\n                </el-select>\r\n              </el-form-item>\r\n            </div>\r\n          </el-col>\r\n        </el-row>\r\n\r\n        <el-row :gutter=\"20\">\r\n          <el-col :span=\"8\">\r\n            <div class=\"grid-content\">\r\n              <el-form-item prop=\"client_vehicle_license\">\r\n                <el-input\r\n                  placeholder=\"Vehicle Number\"\r\n                  v-model=\"form.client_vehicle_license\"\r\n                ></el-input>\r\n              </el-form-item>\r\n            </div>\r\n          </el-col>\r\n          <el-col :span=\"8\">\r\n            <el-form-item prop=\"vehicle_owner\">\r\n              <el-input\r\n                placeholder=\"Vehicle Owner / License\"\r\n                v-model=\"form.vehicle_owner\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"8\">\r\n            <el-form-item prop=\"vehicle_year\">\r\n              <el-input\r\n                placeholder=\"Vehicle Year\"\r\n                v-on:keypress.native=\"isNumber\"\r\n                v-model=\"form.vehicle_year\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n\r\n        <el-row>\r\n          <el-col :span=\"24\">\r\n            <el-col>\r\n              <p>Template & Task</p>\r\n              <hr />\r\n            </el-col>\r\n          </el-col>\r\n        </el-row>\r\n\r\n        <el-row :gutter=\"20\">\r\n          <el-col :span=\"24\">\r\n            <div class=\"grid-content\">\r\n              <el-form-item prop=\"task_template_id\">\r\n                <el-select\r\n                  v-model=\"form.task_template_id\"\r\n                  @change=\"taskT\"\r\n                  :remote-method=\"remoteMethod\"\r\n                  placeholder=\"Select Template\"\r\n                >\r\n                  <el-option\r\n                    v-for=\"item in templateTasks\"\r\n                    :key=\"item.task_template_id\"\r\n                    :label=\"item.task_template_name\"\r\n                    :value=\"item.task_template_id\"\r\n                  ></el-option>\r\n                </el-select>\r\n              </el-form-item>\r\n            </div>\r\n          </el-col>\r\n        </el-row>\r\n\r\n        <el-row>\r\n          <el-col :span=\"24\">\r\n            <el-col>\r\n              <p>\r\n                <b> Description </b>\r\n                <span style=\"padding-left: 1.8em\" v-if=\"data != null\">\r\n                  : {{ data.task_template_description }}</span\r\n                >\r\n              </p>\r\n            </el-col>\r\n          </el-col>\r\n        </el-row>\r\n\r\n        <el-row>\r\n          <el-col :span=\"24\">\r\n            <el-table :data=\"dataTAsk\" stripe style=\"width: 100%\">\r\n              <el-table-column prop=\"name\" label=\"Task Name\"> </el-table-column>\r\n              <el-table-column prop=\"description\" label=\"Task Description\">\r\n              </el-table-column>\r\n              <el-table-column prop=\"updated_at\" label=\"Last Update\">\r\n              </el-table-column>\r\n            </el-table>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col\r\n            :lg=\"{ offset: 18, span: 6 }\"\r\n            style=\"display: flex; justify-content: flex-end; margin-top: 20px\"\r\n          >\r\n            <div class=\"grid-content\">\r\n              <el-form-item>\r\n                <el-button @click=\"isCancel\">Cancel</el-button>\r\n                <el-button\r\n                  type=\"success\"\r\n                  v-loading.fullscreen.lock=\"buttonStatuz\"\r\n                  @click=\"onSubmit('form')\"\r\n                  >Create</el-button\r\n                >\r\n              </el-form-item>\r\n            </div>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from \"vuex\";\r\nimport * as getter from \"../../../store/getters-types\";\r\nimport * as action from \"../../../store/action-types\";\r\nimport * as mutation from \"../../../store/mutation-types\";\r\n\r\nexport default {\r\n  data() {\r\n    let validateVehicleNum = (rule, value, callback) => {\r\n      if (!value.match(/^[A-Z0-9 _]*[A-Z0-9][A-Z0-9 _]*$/g)) {\r\n        callback(new Error(\"Please Input Valid Vehicle Number\"));\r\n      } else {\r\n        callback();\r\n      }\r\n    };\r\n    let validatePhone = (rule, value, callback) => {\r\n      if ((value + \"\").length > 13) {\r\n        callback(new Error(\"Phone must be less than 13 digits\"));\r\n      } else {\r\n        if ((value + \"\").length < 10) {\r\n          callback(new Error(\"Phone must be more than 10 digits\"));\r\n        } else {\r\n          callback();\r\n        }\r\n      }\r\n    };\r\n    return {\r\n      options: null,\r\n      vehicle_brands: null,\r\n      origin: \"\",\r\n      destination: \"\",\r\n      form: {\r\n        fullscreen_loading: false,\r\n        task_template_id: \"\",\r\n        booking_time: \"\",\r\n        origin_latitude: \"\",\r\n        origin_longitude: \"\",\r\n        destination_latitude: \"\",\r\n        destination_longitude: \"\",\r\n        client_vehicle_license: \"\",\r\n        user_fullname: \"\",\r\n        user_phonenumber: \"\",\r\n        vehicle_owner: \"\",\r\n        vehicle_brand: \"\",\r\n        vehicle_type: \"\",\r\n        vehicle_transmission: \"\",\r\n        vehicle_year: \"\",\r\n        vehicle_color: \"\",\r\n        message: \"\",\r\n        order_type: \"\",\r\n        origin_name: \"\",\r\n        destination_name: \"\",\r\n        tasks: null,\r\n      },\r\n      rules: {\r\n        task_template_id: [\r\n          {\r\n            required: true,\r\n            message: \"Please Input Task Template ID\",\r\n            trigger: [\"blur\", \"change\"],\r\n          },\r\n        ],\r\n        booking_time: [\r\n          {\r\n            required: true,\r\n            message: \"Please Input Date & Time\",\r\n            trigger: [\"blur\", \"change\"],\r\n          },\r\n        ],\r\n        origin_latitude: [\r\n          {\r\n            required: true,\r\n            message: \"Please Select Origin\",\r\n            trigger: [\"blur\", \"change\"],\r\n          },\r\n        ],\r\n        destination_latitude: [\r\n          {\r\n            required: true,\r\n            message: \"Please Select Destination\",\r\n            trigger: [\"blur\", \"change\"],\r\n          },\r\n        ],\r\n        client_vehicle_license: [\r\n          {\r\n            required: false,\r\n            message: \"Please Input Vehicle License\",\r\n            trigger: [\"blur\", \"change\"],\r\n          },\r\n          { validator: validateVehicleNum, trigger: [\"change\", \"blur\"] },\r\n        ],\r\n        user_fullname: [\r\n          {\r\n            required: true,\r\n            message: \"Please Input FullName\",\r\n            trigger: [\"blur\", \"change\"],\r\n          },\r\n        ],\r\n        user_phonenumber: [\r\n          {\r\n            required: true,\r\n            message: \"Please Input Phone Number\",\r\n            trigger: [\"blur\", \"change\"],\r\n          },\r\n          { validator: validatePhone, trigger: [\"change\", \"blur\"] },\r\n        ],\r\n        vehicle_owner: [\r\n          {\r\n            required: false,\r\n            message: \"Please Input Vehicle Owner\",\r\n            trigger: [\"blur\", \"change\"],\r\n          },\r\n        ],\r\n        vehicle_brand: [\r\n          {\r\n            required: true,\r\n            message: \"Please Input Vehicle Brand\",\r\n            trigger: [\"blur\", \"change\"],\r\n          },\r\n        ],\r\n        vehicle_type: [\r\n          {\r\n            required: true,\r\n            message: \"Please Input Vehicle Type\",\r\n            trigger: [\"blur\", \"change\"],\r\n          },\r\n        ],\r\n        vehicle_transmission: [\r\n          {\r\n            required: true,\r\n            message: \"Please Input Vehicle Transmission\",\r\n            trigger: [\"blur\", \"change\"],\r\n          },\r\n        ],\r\n        vehicle_year: [\r\n          {\r\n            required: true,\r\n            message: \"Please Input Vehicle Year\",\r\n            trigger: [\"blur\", \"change\"],\r\n          },\r\n        ],\r\n        vehicle_color: [\r\n          {\r\n            required: true,\r\n            message: \"Please Input Vehicle Color\",\r\n            trigger: [\"blur\", \"change\"],\r\n          },\r\n        ],\r\n        message: [\r\n          {\r\n            required: true,\r\n            message: \"Please Input Message\",\r\n            trigger: [\"blur\", \"change\"],\r\n          },\r\n        ]